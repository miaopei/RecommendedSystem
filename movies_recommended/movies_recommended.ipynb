    "#Cosine similarity\n",
    "similarity_usr = train_matrix.dot(train_matrix.T) + 1e-9\n",
    "norms = np.array([np.sqrt(np.diagonal(similarity_usr))])\n",
    "similarity_usr = (similarity_usr / (norms * norms.T))\n",
